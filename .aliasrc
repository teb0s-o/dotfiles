# ls and grep
alias ls='ls --color=tty'
alias grep='grep --color=tty'

# open another terminal in current directory
alias duple="alacritty & disown"

# restart picom
alias picomfix="picom & disown"

# poweroff
alias poweroff='sudo shutdown 00'

# cbonsai
alias cbonsai='cbonsai -li'

# ssh

